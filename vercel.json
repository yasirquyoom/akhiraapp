{
  "version": 2,
  "name": "akhira",
  "public": true,
  "builds": [
    { "src": "build/web/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/build/web/assets/$1" },
    { "src": "/flutter_service_worker.js", "dest": "/build/web/flutter_service_worker.js" },
    { "src": "/(.*)\.(js|css|png|jpg|jpeg|gif|svg|ico|json|wasm|map)", "dest": "/build/web/$1.$2" },
    { "src": "/(.*)", "dest": "/build/web/index.html" }
  ]
}


